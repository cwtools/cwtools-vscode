{
  "name": "cwtools-vscode",
  "displayName": "%extension.displayName%",
  "private": true,
  "description": "%extension.description%",
  "author": "Dayshine",
  "license": "MIT",
  "version": "0.10.26",
  "preview": true,
  "publisher": "tboby",
  "qna": "https://forum.paradoxplaza.com/forum/index.php?threads/tool-cwtools-a-mod-validating-extension-for-vs-code.1066033/",
  "icon": "docs/cwtools_logo.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/cwtools/cwtools-vscode"
  },
  "engines": {
    "vscode": "^1.83.0"
  },
  "categories": [
    "Programming Languages",
    "Linters"
  ],
  "keywords": [
    "paradox",
    "stellaris",
    "victoria",
    "crusader kings",
    "hearts of iron"
  ],
  "main": "./bin/client/extension/extension.js",
  "contributes": {
    "views": {
      "explorer": [
        {
          "id": "cwtools-files",
          "name": "%views.cwtools-files.name%"
        }
      ]
    },
    "menus": {
      "editor/title": [
        {
          "when": "cwtoolsEventFile == true \u0026\u0026 cwtoolsWebview == false",
          "command": "eventGraph",
          "group": "navigation"
        },
        {
          "when": "cwtoolsGraphFile == true \u0026\u0026 cwtoolsWebview == false",
          "command": "showGraph",
          "group": "navigation"
        },
        {
          "when": "cwtoolsWebview == true",
          "command": "saveGraphImage",
          "group": "navigation"
        },
        {
          "when": "cwtoolsWebview == true",
          "command": "setGraphDepth",
          "group": "navigation"
        },
        {
          "when": "cwtoolsWebview == true",
          "command": "saveGraphJson",
          "group": "navigation"
        }
      ]
    },
    "commands": [
      {
        "command": "outputerrors",
        "title": "%commands.outputerrors.title%",
        "category": "cwtools"
      },
      {
        "command": "genlocall",
        "title": "%commands.genlocall.title%",
        "category": "cwtools"
      },
      {
        "command": "reloadrulesconfig",
        "title": "%commands.reloadrulesconfig.title%",
        "category": "cwtools"
      },
      {
        "command": "cacheVanilla",
        "title": "%commands.cacheVanilla.title%",
        "category": "cwtools"
      },
      {
        "command": "listAllFiles",
        "title": "%commands.listAllFiles.title%",
        "category": "cwtools"
      },
      {
        "command": "listAllLocFiles",
        "title": "%commands.listAllLocFiles.title%",
        "category": "cwtools"
      },
      {
        "command": "debugrules",
        "title": "%commands.debugrules.title%",
        "category": "cwtools"
      },
      {
        "command": "pretriggerAllFiles",
        "title": "%commands.pretriggerAllFiles.title%",
        "category": "cwtools"
      },
      {
        "command": "startcustomgame",
        "title": "%commands.startcustomgame.title%",
        "category": "cwtools"
      },
      {
        "command": "techGraph",
        "title": "%commands.techGraph.title%",
        "category": "cwtools"
      },
      {
        "command": "showGraph",
        "title": "%commands.showGraph.title%",
        "category": "cwtools"
      },
      {
        "command": "eventGraph",
        "title": "%commands.eventGraph.title%",
        "category": "cwtools"
      },
      {
        "command": "graphFromJson",
        "title": "%commands.graphFromJson.title%",
        "category": "cwtools"
      },
      {
        "command": "exportTypes",
        "title": "%commands.exportTypes.title%",
        "category": "cwtools"
      },
      {
        "command": "saveGraphImage",
        "title": "%commands.saveGraphImage.title%",
        "category": "cwtools",
        "enablement": "cwtoolsWebview == true"
      },
      {
        "command": "setGraphDepth",
        "title": "%commands.setGraphDepth.title%",
        "category": "cwtools",
        "enablement": "cwtoolsWebview == true"
      },
      {
        "command": "saveGraphJson",
        "title": "%commands.saveGraphJson.title%",
        "category": "cwtools",
        "enablement": "cwtoolsWebview == true"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "%configuration.title%",
      "properties": {
        "cwtools.trace.server": {
          "scope": "window",
          "type": "string",
          "enum": [
            "off",
            "messages",
            "verbose"
          ],
          "default": "off",
          "description": "%configuration.cwtools.trace.server.description%"
        },
        "cwtools.localisation.languages": {
          "scope": "window",
          "type": "array",
          "items": {
            "type": "string",
            "enum": [
              "English",
              "French",
              "German",
              "Spanish",
              "Russian",
              "Braz_Por",
              "Polish",
              "Chinese",
              "Korean",
              "Japanese",
              "Turkish"
            ]
          },
          "default": [
            "English"
          ],
          "description": "%configuration.cwtools.localisation.languages.description%",
          "uniqueItems": true
        },
        "cwtools.localisation.generated_strings": {
          "scope": "window",
          "type": "string",
          "default": ":0 \u0022REPLACE_ME\u0022",
          "description": "%configuration.cwtools.localisation.generated_strings.description%"
        },
        "cwtools.errors.vanilla": {
          "scope": "window",
          "type": "boolean",
          "default": false,
          "description": "%configuration.cwtools.errors.vanilla.description%"
        },
        "cwtools.errors.ignore": {
          "scope": "window",
          "type": "array",
          "description": "%configuration.cwtools.errors.ignore.description%",
          "uniqueItems": true
        },
        "cwtools.errors.ignorefiles": {
          "scope": "window",
          "type": "array",
          "description": "%configuration.cwtools.errors.ignorefiles.description%",
          "uniqueItems": true,
          "default": [
            "README.txt",
            "credits.txt",
            "credits_l_simp_chinese.txt",
            "reference.txt",
            "startup_info.txt"
          ]
        },
        "cwtools.experimental": {
          "scope": "window",
          "type": "boolean",
          "default": true,
          "description": "%configuration.cwtools.experimental.description%"
        },
        "cwtools.debug_mode": {
          "scope": "window",
          "type": "boolean",
          "default": true,
          "description": "%configuration.cwtools.debug_mode.description%"
        },
        "cwtools.logging.diagnostic": {
          "scope": "window",
          "type": "boolean",
          "default": false,
          "description": "%configuration.cwtools.logging.diagnostic.description%"
        },
        "cwtools.rules_version": {
          "scope": "window",
          "type": "string",
          "enum": [
            "stable",
            "latest",
            "manual"
          ],
          "default": "latest",
          "description": "%configuration.cwtools.rules_version.description%"
        },
        "cwtools.rules_folder": {
          "scope": "window",
          "type": "string",
          "description": "%configuration.cwtools.rules_folder.description%"
        },
        "cwtools.ignore_patterns": {
          "scope": "window",
          "type": "array",
          "description": "%configuration.cwtools.ignore_patterns.description%",
          "uniqueItems": true,
          "default": [
            "**/99_README**.txt"
          ]
        },
        "cwtools.cache.eu4": {
          "scope": "application",
          "type": "string",
          "description": "%configuration.cwtools.cache.eu4.description%"
        },
        "cwtools.cache.hoi4": {
          "scope": "application",
          "type": "string",
          "description": "%configuration.cwtools.cache.hoi4.description%"
        },
        "cwtools.cache.stellaris": {
          "scope": "application",
          "type": "string",
          "description": "%configuration.cwtools.cache.stellaris.description%"
        },
        "cwtools.cache.ck2": {
          "scope": "application",
          "type": "string",
          "description": "%configuration.cwtools.cache.ck2.description%"
        },
        "cwtools.cache.imperator": {
          "scope": "application",
          "type": "string",
          "description": "%configuration.cwtools.cache.imperator.description%"
        },
        "cwtools.cache.vic2": {
          "scope": "application",
          "type": "string",
          "description": "%configuration.cwtools.cache.vic2.description%"
        },
        "cwtools.cache.ck3": {
          "scope": "application",
          "type": "string",
          "description": "%configuration.cwtools.cache.ck3.description%"
        },
        "cwtools.cache.vic3": {
          "scope": "application",
          "type": "string",
          "description": "%configuration.cwtools.cache.vic3.description%"
        },
        "cwtools.graph.zoomSensitivity": {
          "scope": "resource",
          "type": "number",
          "maximum": 2,
          "minimum": 0,
          "description": "%configuration.cwtools.graph.zoomSensitivity.description%",
          "default": 1
        },
        "cwtools.maxFileSize": {
          "scope": "resource",
          "type": "integer",
          "minimum": 1,
          "default": 2,
          "description": "%configuration.cwtools.maxFileSize.description%"
        },
        "cwtools.localisation.replaceme": {
          "scope": "window",
          "type": "string",
          "description": "%configuration.cwtools.localisation.replaceme.description%"
        }
      }
    },
    "languages": [
      {
        "id": "stellaris",
        "aliases": [
          "",
          "paradox",
          "pdxscript"
        ],
        "extensions": [
          "txt",
          "gui",
          "gfx",
          "asset",
          "cwt"
        ],
        "configuration": "./language-configuration.json",
        "filenamePatterns": [
