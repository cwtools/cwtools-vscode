    "configurationDefaults": {
      "[stellaris]": {
        "files.autoGuessEncoding": true,
        "editor.quickSuggestions": {
          "other": true,
          "comments": false,
          "strings": true
        },
        "editor.insertSpaces": false,
        "editor.acceptSuggestionOnEnter": "off",
