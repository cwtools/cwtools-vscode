        "snippets": [
            {
                "language": "stellaris",
                "path": "./snippets/paradox.json"
            }
        ],
        "configurationDefaults": {
            "[stellaris]": {
                "files.autoGuessEncoding": true,
                "editor.quickSuggestions": {
                    "other": true,
                    "comments": false,
                    "strings": true
                },
                "editor.insertSpaces": false,
                "editor.acceptSuggestionOnEnter": "off"
            },
            "[hoi4]": {
                "files.autoGuessEncoding": true,
                "editor.quickSuggestions": {
                    "other": true,
                    "comments": false,
                    "strings": true
                },
                "editor.insertSpaces": false,
                "editor.acceptSuggestionOnEnter": "off"
            },
            "[eu4]": {
                "files.autoGuessEncoding": true,
                "editor.quickSuggestions": {
                    "other": true,
