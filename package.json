        "popper.js": "^1.15.0",
        "tippy.js": "^4.3.5",
        "merge-images": "1.2.0",
        "@types/handlebars": "^4.0.40",
        "@types/mocha": "^5.2.7",
        "@types/node": "^8.10.63",
        "@types/qtip2": "^2.2.30",
        "@types/cytoscape": "3.14.7",
        "@types/vscode": "^1.45.0",
        "@types/glob": "^7.1.3",
        "copyfiles": "^2.3.0",
        "rollup-plugin-commonjs": "^10.0.2",
